      &lt;use xlink:href=&quot;#corner&quot; x=&quot;0&quot; y=&quot;0&quot; class=&quot;svg-corner&quot;/&gt;
    &lt;/svg&gt;
    &lt;svg viewBox=&quot;0 0 100 32&quot; preserveAspectRatio=&quot;none&quot; width=&quot;100%&quot;&gt;
      &lt;use xlink:href=&quot;#horizontal-top&quot; x=&quot;0&quot; y=&quot;0&quot; class=&quot;svg-item&quot;/&gt;
    &lt;/svg&gt;
    &lt;svg&gt;
      &lt;use xlink:href=&quot;#corner&quot; x=&quot;0&quot; y=&quot;0&quot; class=&quot;svg-corner&quot;/&gt;
    &lt;/svg&gt;
    &lt;svg viewBox=&quot;0 0 32 100&quot; preserveAspectRatio=&quot;none&quot; height=&quot;100%&quot;&gt;
      &lt;use xlink:href=&quot;#vertical&quot; x=&quot;0&quot; y=&quot;0&quot; class=&quot;svg-item&quot;/&gt;
    &lt;/svg&gt;
    &lt;div style=&quot;min-height: 4rem; padding: 1rem;&quot;&gt;
      &lt;h3 style=&quot;text-align: center&quot;&gt;Centre content&lt;/h3&gt;
      &lt;p&gt;More content&lt;/p&gt;
      &lt;p&gt;More content&lt;/p&gt;
    &lt;/div&gt;
    &lt;svg viewBox=&quot;0 0 32 100&quot; preserveAspectRatio=&quot;none&quot; height=&quot;100%&quot;&gt;
      &lt;use xlink:href=&quot;#vertical&quot; x=&quot;0&quot; y=&quot;0&quot; class=&quot;svg-item&quot;/&gt;
    &lt;/svg&gt;
    &lt;svg&gt;
      &lt;use xlink:href=&quot;#corner&quot; x=&quot;0&quot; y=&quot;0&quot; class=&quot;svg-corner&quot;/&gt;
    &lt;/svg&gt;
    &lt;svg viewBox=&quot;0 0 100 32&quot; preserveAspectRatio=&quot;none&quot; width=&quot;100%&quot;&gt;
      &lt;use xlink:href=&quot;#horizontal-bottom&quot; x=&quot;0&quot; y=&quot;0&quot; class=&quot;svg-item&quot; /&gt;
    &lt;/svg&gt;
    &lt;svg&gt;
      &lt;use xlink:href=&quot;#corner&quot; x=&quot;0&quot; y=&quot;0&quot; class=&quot;svg-corner&quot;/&gt;
    &lt;/svg&gt;
  &lt;/div&gt;
  &lt;/div&gt;
